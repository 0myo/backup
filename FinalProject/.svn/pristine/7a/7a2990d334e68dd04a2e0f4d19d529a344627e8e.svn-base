<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="Prof-Mapper">

	<select id="selectProfByProfId" parameterType="String" resultType="prof">
		select *
		from PROF
		where PROF_CD=#{profId}
	</select>
	
</mapper>