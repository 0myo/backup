<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="MajorChange-Mapper">

	<select id="selectMajorChange" parameterType="String" resultType="majorChange">
		select *
		from major_change
		where stu_cd = #{stuCd}
	</select>
	
	
	
</mapper>