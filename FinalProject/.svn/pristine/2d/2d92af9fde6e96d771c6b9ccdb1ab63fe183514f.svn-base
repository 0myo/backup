<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="StuMajor-Mapper">

	<select id="selectStuMajorList" parameterType="String" resultType="stuMajor">
		select * from stu_major
		where stu_id=#{stuId}
	</select>
	
	
	
</mapper>