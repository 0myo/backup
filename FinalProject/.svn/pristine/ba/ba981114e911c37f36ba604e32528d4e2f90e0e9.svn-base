<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Student-Mapper">
	<select id="selectInfo" parameterType="String" resultType="student">
		select
		*
		from student
		where stu_cd = #{stuCd}
	</select>
	
	
	
	
</mapper>